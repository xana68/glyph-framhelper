  /
    Links
    \==================================================
  .example
    .headline.headline-info
      %h2 Links
    .row
      .col-md-3
        .panel.panel-default
          .panel-heading
            Default link
          .panel-body
            = link_to "Link", "#", class: ""

        .panel.panel-info
          .panel-heading
            = fa_icon 'exclamation'
            Juste for no-touch
          .panel-body  
            = link_to "sliding-middle-out", "#", class: "sliding-middle-out"  
       
      .col-md-3
        .panel.panel-default
          .panel-heading
            Default image link
          .panel-body
            = link_to "#", class: "link-img" do
              %img{ :"data-src" => "holder.js/100%x100%", alt: "100%x100%", src: "data:image/svg+xml;base64,", style: "height: 100%; width: 100%; display: block;" }

      .col-md-3
        .panel.panel-info
          .panel-heading
            = fa_icon 'exclamation'
            Juste for no-touch
          .panel-body  
            = link_to "#", class: "link-img with-icon" do
              = fa_stacked_icon "link inverse", base: "square", class: "icon-link"
              %img{ :"data-src" => "holder.js/100%x100%", alt: "100%x100%", src: "data:image/svg+xml;base64,", style: "height: 100%; width: 100%; display: block;" }    

      .col-md-3
        .panel.panel-info
          .panel-heading
            = fa_icon 'exclamation'
            Juste for no-touch
          .panel-body  
            = link_to "#", class: "link-img zoomer" do
              .overlay-zoom
                .overlay-icon
                  = fa_icon "search"
                %img{ :"data-src" => "holder.js/100%x170", alt: "100%x170", src: "data:image/svg+xml;base64,", style: "height: 100%; width: 100%; display: block;" }    

