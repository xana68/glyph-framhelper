  /
    Dropdowns
    \==================================================        
  .example    
    .row
      .col-md-4
        .headline.headline-primary
          %h2 Dropdowns

        .dropdown 
          %button.btn.btn-default{ data: { toggle: "dropdown" } }
            Dropdowns
            %b.caret  
          %ul.dropdown-menu{role: "menu"}
            %li.dropdown-header Setting
            %li{role: "presentation"}
              %a{href: "#", role: "menuitem", tabindex: "-1"} Action
            %li.disabled{role: "presentation"}
              %a{href: "#", role: "menuitem", tabindex: "-1"} Another action
            %li.dropdown-submenu
              %a{href: "#"} Something else here
              %ul.dropdown-menu
                %li
                  %a{href: "#"} Action1
                %li
                  %a{href: "#"} Action2
                %li
                  %a{href: "#"} Another action
                %li
                  %a{href: "#"} Something else here
                %li.divider
                %li
                  %a{href: "#"} Separated link
            %li.divider{role: "presentation"}
            %li{role: "presentation"}
              %a{href: "#", role: "menuitem", tabindex: "-1"} Separated link

      .col-md-4
        .headline.headline-primary
          %h2
            %a{ href: "https://github.com/CWSpear/bootstrap-hover-dropdown", target: "_blank" }
